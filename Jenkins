pipeline {
   agent any
   stages {

        stage("Git checkout"){
          steps {
           git credentialsId: 'github-cred', url: 'https://github.com/janardhan-0348/devopsclsrepo26.git'
    }
        }
          
           stage("Maven build"){
              steps {
               sh 'mvn clean package'
               sh "mv target/*.war target/myweb.war"  
              }
           }
      
     
      
      
      }
           
}

}
